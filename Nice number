#include <iostream>
using namespace std;

int main() {
    int num;
    cout<<"Enter a number:";
    cin>>num;
    if (num<0){
        cout<<"The number is negative.";
    }else if (num % 10==10){
        cout<<"The number is divisible by 10.";\
    } else if (num % 5 == 0){
        cout<<"The number is divisible by 5.";
    } else{
        cout<<"Nice number.";
    }
    return 0;
}
